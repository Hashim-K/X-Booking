# X-Booking Configuration

# Multi-booking mode
# Set to "true" to enable booking for multiple users in parallel
MULTI_BOOKING=false

# Path to JSON file containing multiple user credentials
# Only used when MULTI_BOOKING=true
MULTI_BOOKING_FILE=./bookings.json

# University/Institution credentials (for BookingClient)
# IMPORTANT: Don't use USERNAME/PASSWORD as they conflict with OS environment variables
# Use TU_USERNAME and TU_PASSWORD instead
TU_USERNAME=your_netid_here
TU_PASSWORD=your_password_here

# Institution selection (optional, defaults to 'tudelft')
# Options: tudelft, inholland, thuas
INSTITUTION=tudelft

# Server configuration
PORT=8008
HOST=0.0.0.0

# Browser configuration (optional)
# Leave empty to use system default browser detection
# Options: brave, chrome, chromium
BROWSER=
